mode: 2d      # 1d or 2d
Nmesh: 256     # size of mesh

field:
    DataSource:
        plugin: Pandas
        path: ../data/pandas_data.hdf5
        names: data
        BoxSize: 1380.
        poscols: [x, y, z]
        velcols: [vx, vy, vz]
        posf: 1380.0
        velf: 1380.0
        rsd: z
    Transfer: [NormalizeDC, RemoveDC, AnisotropicCIC]

output: ../output/power_2d_pandas_hdf.dat  # output